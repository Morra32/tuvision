<script id="RecsTemplate" type="text/template">
	
    	{{#items}}
        	{{#if imageURL}}
  			<div class="certona-result certona-result-lazy">
              <a href="/products/{{ handle }}?variant={{ variantID }}&rrec=true" class="certona-img">
              	<img data-lazy="{{addQuayImgSize imageURL }}" alt="Product Recommendation - {{ title }}" class="product-featured-image">
              </a>
              <a href="/products/{{ handle }}?variant={{ variantID }}&rrec=true" class="certona-link">
                <p class="name_wrapper">
                  {{{ title }}} / <span class="money">{{ priceRegular }}</span>
                </p>
              </a>
              <button type="submit" name="add" class="btn btn--clear uppercase btn-position-white btn-certona RecsAddToCart">
                <span id="AddToCartText">ADD TO BAG</span>
                <input type="hidden" class="RecsId" name="id" value="{{ variantID }}" />
								{{#ifNotEqual priceCompareTo "$"}}
								<input type="hidden" name="properties[Final Sale]" value="true" />
								{{/ifNotEqual}}
              </button>
          	</div>
            {{/if}}
        {{/items}}
	
</script>

<script id="RecsTemplateNotLazy" type="text/template">
	
    	{{#items}}
        	{{#if imageURL}}
  			<div class="certona-result">
              <a href="/products/{{ handle }}?variant={{ variantID }}&rrec=true" class="certona-img">
              	<img src="{{addQuayImgSize imageURL}}" alt="Product Recommendation - {{ title }}" class="product-featured-image">
              </a>
              <a href="/products/{{ handle }}?variant={{ variantID }}&rrec=true" class="certona-link">
                <p class="name_wrapper">
                  {{{ title }}} / <span class="money">{{ priceRegular }}</span>
                </p>
              </a>
              <button type="submit" name="add" class="btn btn--clear uppercase btn-position-white btn-certona RecsAddToCart">
                <span id="AddToCartText">ADD TO BAG</span>
                <input type="hidden" class="RecsId" name="id" value="{{ variantID }}" />
								{{#ifNotEqual priceCompareTo "$"}}
								<input type="hidden" name="properties[Final Sale]" value="true" />
								{{/ifNotEqual}}
              </button>
          	</div>
            {{/if}}
        {{/items}}
	
</script>

<script id="DittoRecsTemplate" type="text/template">
	
    	{{#items}}

  			<div class="ditto-result ditto-result-lazy">

