      clickEU = currentUrlStripped.replace(baseUrlUS_PROD, baseUrlEU_PROD) + paramGeo;
    } else if (currentUrl.indexOf(baseUrlUK_PROD) > -1) {
      clickAU = currentUrlStripped.replace(baseUrlUK_PROD, baseUrlAU_PROD) + paramGeo;
      clickUS = currentUrlStripped.replace(baseUrlUK_PROD, baseUrlUS_PROD) + paramGeo;
      clickEU = currentUrlStripped.replace(baseUrlUK_PROD, baseUrlEU_PROD) + paramGeo;
    } else if (currentUrl.indexOf(baseUrlEU_PROD) > -1) {
      clickAU = currentUrlStripped.replace(baseUrlEU_PROD, baseUrlAU_PROD) + paramGeo;
      clickUS = currentUrlStripped.replace(baseUrlEU_PROD, baseUrlUS_PROD) + paramGeo;
      clickUK = currentUrlStripped.replace(baseUrlEU_PROD, baseUrlUK_PROD) + paramGeo;
    }else if (currentUrl.indexOf(baseUrlAU_DEV) > -1) {
      clickUS = currentUrlStripped.replace(baseUrlAU_DEV, baseUrlUS_DEV) + paramGeo;
      clickUK = currentUrlStripped.replace(baseUrlAU_DEV, baseUrlUK_DEV) + paramGeo;
      clickEU = currentUrlStripped.replace(baseUrlAU_DEV, baseUrlEU_DEV) + paramGeo;
    } else if (currentUrl.indexOf(baseUrlUS_DEV) > -1) {
      clickAU = currentUrlStripped.replace(baseUrlUS_DEV, baseUrlAU_DEV) + paramGeo;
      clickUK = currentUrlStripped.replace(baseUrlUS_DEV, baseUrlUK_DEV) + paramGeo;
      clickEU = currentUrlStripped.replace(baseUrlUS_DEV, baseUrlEU_DEV) + paramGeo;
    } else if (currentUrl.indexOf(baseUrlUK_DEV) > -1) {
      clickAU = currentUrlStripped.replace(baseUrlUK_DEV, baseUrlAU_DEV) + paramGeo;
      clickUS = currentUrlStripped.replace(baseUrlUK_DEV, baseUrlUS_DEV) + paramGeo;
      clickEU = currentUrlStripped.replace(baseUrlUK_DEV, baseUrlEU_DEV) + paramGeo;
    } else if (currentUrl.indexOf(baseUrlEU_DEV) > -1) {
      clickAU = currentUrlStripped.replace(baseUrlEU_DEV, baseUrlAU_DEV) + paramGeo;
      clickUS = currentUrlStripped.replace(baseUrlEU_DEV, baseUrlUS_DEV) + paramGeo;
      clickUK = currentUrlStripped.replace(baseUrlEU_DEV, baseUrlUK_DEV) + paramGeo;
    }
