<script data-name="locationmap">
  // @ref: https://github.com/js-cookie/js-cookie
  // @ref: http://stackoverflow.com/questions/6362688/jquery-check-if-cookie-exists-if-not-create-it

  $(document).ready(function() {

    // VARS
    var cookieName = "quay_site_loc",
        cookieValue = Cookies.get(cookieName),
        currentUrl = window.location.href,
        currentUrlStripped = currentUrl,
        currentDomain = "",
        cookieNameDismissed = "quay_site_loc_dismissed",
        cookieValueDismissed = Cookies.get(cookieNameDismissed),
        baseUrlAU_PROD = "quayaustralia.com.au/",
        baseUrlUS_PROD = "quayaustralia.com/",
        baseUrlUK_PROD = "quayaustralia.co.uk/",
        baseUrlEU_PROD = "quayaustralia.eu/",
        baseUrlEU_FR = "quayaustralia.fr",
        baseUrlEU_IT = "quayaustralia.it",
        baseUrlEU_ES = "quayaustralia.es",
        baseUrlEU_DE = "quayaustralia.de",
        baseUrlAU_DEV = "quayaustralia.myshopify.com/",
        baseUrlUS_DEV = "quayusa.myshopify.com/",
        baseUrlUK_DEV = "quayuk.myshopify.com/",
        baseUrlEU_DEV = "quayeurope.myshopify.com/",
        baseUrlEU_Sandbox = "quayeuropesbx.myshopify.com/",
        baseUrlSandbox = "quaysandbox.myshopify.com/",
        paramAU = "AU",
        paramUS = "US",
        paramUK = "UK",
        paramEU = "EU",
        paramGeo = "geo=1",
        removeGeo = "geo=0",
        clickAU = "",
        clickUS = "",
        clickUK = "",
        clickEU = "https://www.quayaustralia.eu",
        showModal = false,
        locModalElements = $("#LocationModal").find("a"),
        locModalLastClick = undefined,
        is_test = false;

    // SET CURRENT DOMAIN VARIABLE
    // set which domain we're on
    if ((currentUrl.indexOf(baseUrlAU_PROD) > -1) || (currentUrl.indexOf(baseUrlAU_DEV) > -1)) {
      currentDomain = paramAU;
    } else if ((currentUrl.indexOf(baseUrlUS_PROD) > -1) || (currentUrl.indexOf(baseUrlUS_DEV) > -1) || (currentUrl.indexOf(baseUrlSandbox) > -1)) {
      currentDomain = paramUS;
    } else if ((currentUrl.indexOf(baseUrlEU_PROD) > -1) || (currentUrl.indexOf(baseUrlEU_DEV) > -1) || (currentUrl.indexOf(baseUrlEU_Sandbox) > -1) || (currentUrl.indexOf(baseUrlEU_FR) > -1) || (currentUrl.indexOf(baseUrlEU_IT) > -1) || (currentUrl.indexOf(baseUrlEU_ES) > -1) || (currentUrl.indexOf(baseUrlEU_DE) > -1)) {
      currentDomain = paramEU;
    } else if ((currentUrl.indexOf(baseUrlUK_PROD) > -1) || (currentUrl.indexOf(baseUrlUK_DEV) > -1)) {
      currentDomain = paramUK;
    }

    // SET TEST VARIABLE
    if (currentUrl.indexOf(baseUrlSandbox) > -1) {
      is_test = true;
    }

    // COOKIE FUNCTIONS
    function removeCookie(name) {Cookies.remove(name);}
    function setCookie(value, name) {Cookies.set(name, value, {expires : 100000});}
    function setCookieLimit(value, name, time) {Cookies.set(name, value, {expires: time });}

    // CHECK FOR SOCIAL PARAMETERS
    function getParameterByName(name, url) {
      if (!url) url = window.location.href;
      name = name.replace(/[\[\]]/g, "\\$&");
      var regex = new RegExp("[?&]" + name + "(=([^&#]*)|&|#|$)"),
          results = regex.exec(url);
      if (!results) return null;
      if (!results[2]) return '';
      return decodeURIComponent(results[2].replace(/\+/g, " "));
    }

    function checkForSocialTags(){
      if (currentUrlStripped.indexOf("?") > -1) {

        var hasMedium = true;
        var socialMedium = "";
        var socialSrc = "";
        try{
          socialMedium = getParameterByName("utm_medium", currentUrlStripped).toLowerCase();
          socialSrc = getParameterByName("utm_source", currentUrlStripped).toLowerCase();

          if(socialMedium == "affiliate"){
          	return true;
          } else if(socialMedium == "referral"){
            if((socialSrc.indexOf("fb") > -1) || (socialSrc.indexOf("facebook") > -1) || (socialSrc.indexOf("ig") > -1) || (socialSrc.indexOf("instagram") > -1) || (socialSrc.indexOf("twitter") > -1) || (socialSrc.indexOf("pinterest") > -1)){
            	return true;
            }
          }
        }
        catch (err){
          hasMedium = false;
          return false;
        }
      }

      return false;
    }

    

    // SET COOKIE IF GEO TAG EXISTS
    // see if geo is set (geo is set on map click)
    if (currentUrl.indexOf(paramGeo) > -1) {

      // check which domain we're on a set a cookie
      if (currentDomain === paramAU) {
        setCookie(paramAU, cookieName);
      } else if (currentDomain === paramUS) {
        setCookie(paramUS, cookieName);
      } else if (currentDomain === paramEU){
      	setCookie(paramEU, cookieName);
      } else if (currentDomain === paramUK) {
        setCookie(paramUK, cookieName);
      }

      showModal = false;

    } else if (currentUrl.indexOf(removeGeo) > -1) {

      removeCookie(cookieName);
      showModal = true;

    } else if (currentDomain !== cookieValue) {

      if(!cookieValue){
        if(!checkForSocialTags()){
          // we have no geotag. set it to domain landed on by default
          if (currentDomain === paramAU) {
              setCookie(paramAU, cookieName);
          } else if (currentDomain === paramUS) {
            setCookie(paramUS, cookieName);
          } else if (currentDomain === paramEU) {
            setCookie(paramEU, cookieName);
          } else if (currentDomain === paramUK) {
            setCookie(paramUK, cookieName);
          }
          cookieValue = Cookies.get(cookieName);
        } else {
        	showModal = true;
        }
      }
      else{
        //  do something if we landed on a domain that doesn't equal our cookie value
      	showModal = true;
      }

    }

    // STRIP URL TO SET LINKS
    // we're removing url parameters we set in this code to keep our urls and checks clean
    function checkUrlParams() {
      //if (is_test == true) console.log("<F> checkUrlParams");
      if (currentUrlStripped.indexOf("&geo=") > -1) {
        removeUrlGeo();
      } else if (currentUrlStripped.indexOf("?geo=") > -1) {
        removeUrlGeo2();
      } else if (currentUrlStripped.indexOf("#LocationModal") > -1) {
        removeUrlMap();
      } else {
        //if (is_test == true) console.log("<F> checkUrlParams STOP");
        return currentUrlStripped;
      }
    }
    function removeUrlGeo() {
      if (is_test == true) console.log("<F> removeUrlGeo");
      currentUrlStripped = currentUrlStripped.replace("&geo=1", "").replace("&geo=0", "");
      checkUrlParams();
    }
    function removeUrlGeo2() {
      if (is_test == true) console.log("<F> removeUrlGeo2");
      currentUrlStripped = currentUrlStripped.replace("?geo=1&", "?").replace("?geo=0&", "?");
      currentUrlStripped = currentUrlStripped.replace("?geo=1", "").replace("?geo=0", "");
      checkUrlParams();
    }
    function removeUrlMap() {
      if (is_test == true) console.log("<F> removeUrlMap");
      currentUrlStripped = currentUrlStripped.replace("#LocationModal", "");
      checkUrlParams();
    }
    // actually run this script
    checkUrlParams();


    // GEO PARAMETER PREFIX
    // prefix parameter with ? or & based on url
    if (currentUrlStripped.indexOf("?") > -1) {
      paramGeo = "&" + paramGeo;
      removeGeo = "&" + removeGeo;
    } else {
      paramGeo = "?" + paramGeo;
      removeGeo = "?" + removeGeo;
    }


    // SET CURRENT DOMAIN VARIABLE WITH URL CHECK
    // set click link for opposite location
    if (currentUrl.indexOf(baseUrlAU_PROD) > -1) {
      clickUS = currentUrlStripped.replace(baseUrlAU_PROD, baseUrlUS_PROD) + paramGeo;
      clickUK = currentUrlStripped.replace(baseUrlAU_PROD, baseUrlUK_PROD) + paramGeo;
      clickEU = currentUrlStripped.replace(baseUrlAU_PROD, baseUrlEU_PROD) + paramGeo;
