                <p class="name_wrapper">
                  {{ title }} / <span class="money">{{ price }}</span>
                </p>
              </a>
							<button class="ditto-try-on-link" data-sku="{{ sku }}" data-variant="{{ vid }}" data-prod="{{ title }}" data-price="{{ price }}">TRY ON</button>
							<a href="/products/{{ handle }}?variant={{ vid }}" class="ditto-link">SEE MORE COLORS</a>
      	</div>

        {{/items}}
	
</script>

    
