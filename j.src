    // CLICK FUNCTIONS
    // close modal and remove error-state
    function closeLocationModal() {
      $("#LocationModal, .quay-modal__bg").fadeOut();
      locModalLastClick.focus();
    }

    function accessibleLocationModal(e){
      var KEY_TAB = 9;
      var KEY_ESC = 27;

      function handleBackwardTab() {
        if (document.activeElement === locModalElements[0]  ) {
          e.preventDefault();
          locModalElements[locModalElements.length - 1].focus();
        }
      }
      function handleForwardTab() {
        if ( document.activeElement === locModalElements[locModalElements.length -  1] ) {
          e.preventDefault();
          locModalElements[0].focus();
        }
      }

      switch(e.keyCode) {
        case KEY_TAB:
          if (locModalElements.length === 1 ) {
            e.preventDefault();
            break;
          }
          if ( e.shiftKey ) {
            handleBackwardTab();
          } else {
            handleForwardTab();
          }
          break;
        case KEY_ESC:
          closeLocationModal();
          break;
        default:
          break;
      }
    }

    function openLocationModal(){
      $("#LocationModal, .quay-modal__bg").fadeIn();
      locModalLastClick = document.activeElement;
      if(locModalElements[0]){
      	locModalElements[0].addEventListener('keydown', function(e){ accessibleLocationModal(e); });;
        locModalElements[0].focus();
      }
      quayLazyLoad.update();
    }

    // SET BACKGROUND CLICKABLE
    // if current site is cookied, close the modal on bg click
    // otherwise, show an error state to highlight a must-choose
    $(".close_location_modal").click(function(e) {
      e.preventDefault();
      if (currentDomain === cookieValue) {
        closeLocationModal();
      } else {
        setCookieLimit(true, cookieNameDismissed, 1);
        closeLocationModal();
        //$("#LocationModal").addClass("error-state");
      }
    });

    // SET COOKIES BASED ON MAP CLICK
    // click on the AU side, close modal and set cookie
    // click on NON AU, use set url link for aprorpriate click
    $("#LocationModal #AU").click(function(e) {
      e.preventDefault();
      setCookie(paramAU, cookieName);
      if (currentDomain === paramAU) {
        closeLocationModal();
      } else {
        window.location = clickAU;
      }
    });
    // click on the US side, close modal and set cookie
    // click on NON US, use set url link for aprorpriate click
    $("#LocationModal #US").click(function(e) {
      e.preventDefault();
      setCookie(paramUS, cookieName);
      if (currentDomain === paramUS) {
        closeLocationModal();
      } else {
        window.location = clickUS;
      }
    });
    // click on the UK side, close modal and set cookie
    // click on NON UK, use set url link for aprorpriate click
    $("#LocationModal #UK").click(function(e) {
      e.preventDefault();
      setCookie(paramUK, cookieName);
      if (currentDomain === paramUK) {
        closeLocationModal();
      } else {
        window.location = clickUK;
      }
    });
    // click on the EU side, close modal and set cookie
    // click on NON EU, use set url link for aprorpriate click
    $("#LocationModal .back_location_modal").click(function(e) {
      e.preventDefault();
      $("#LocationModal #language-selector").hide();
      $("#LocationModal #region-selector").show();
    });
    $("#LocationModal #click-EU").click(function(e) {
      e.preventDefault();
      $("#LocationModal #region-selector").hide();
      $("#LocationModal #language-selector").show();
    });

    // click on the EU side, close modal and set cookie
    // click on NON EU, use set url link for aprorpriate click
    $("#LocationModal #EU").click(function(e) {
      e.preventDefault();
      setCookie(paramEU, cookieName);
      if (currentDomain === paramEU) {
        if(window.location.href.indexOf("eu") > -1){
        	closeLocationModal();
        } else {
          	window.location = $("#LocationModal #EU").attr("href");
        }
      } else {
        window.location = clickEU;
      }
    });

    // OPEN MODAL
    // if show modal is true
    if (showModal === true ) {
      if(!cookieValueDismissed){
      	openLocationModal();
      }
    }
    // manually with click

    $(".header-flag, a[href='#LocationModal']").on('click', function() {
      openLocationModal();
    });
  });

</script>

